mkdir laz
cd laz

for i in "${flights[@]}"
do
	wget --no-check-certificate $i &
	wait;
done

echo "Finished Downloading Dublin Dataset"
echo "now extracting..."

for laz_zip in *
do 
	echo "$laz_zip"
	unzip "$laz_zip"
	rm "$laz_zip"
done

for laz_dir in *
do
	cd $laz_dir
	mv * ..
	cd ..
	rm -rf $laz_dir
done

